{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "lib": [
      "esnext",
    ],
    "baseUrl": ".",
    "types": [
      "node",
      "vue",
      "google-libphonenumber"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
